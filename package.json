  "bugs": {
    "url": "https://github.com/akshaypatel99/mercado-api/issues"
  },
  "homepage": "https://github.com/akshaypatel99/mercado-api#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.16",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/nodemon": "^1.19.1",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
